# Copy this file to .env and fill in your credentials
# .env should be gitignored - never commit secrets

# API token for Configuration API (required for plan/apply)
NCO_API_TOKEN=your-api-token-here

# Norce Commerce credentials
NORCE_IDENTITY_SECRET=your-identity-secret

# Walley credentials
WALLEY_SCOPE=your-scope
WALLEY_STORE_ID_SE=your-sweden-store-id
WALLEY_STORE_ID_NO=your-norway-store-id
WALLEY_CLIENT_ID=your-client-id
WALLEY_API_SECRET=your-api-secret
